{% extends 'base.html.twig' %}

{% block title %}Creating WM{% endblock %}

{% block body %}
    {% include('inc/navbarWM.html.twig') %}
    <script src="{{ asset('js/show_process.js') }}"></script>
    <script src="{{ asset('js/date_picker.js') }}"></script>
    <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css"
          rel="stylesheet">
    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    <script src="https://code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
    <div class="form-group mx-sm-3 mb-2">
        {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.image) }}
        {{ form_row(form.category) }}
        {{ form_row(form.description) }}
        {{ form_row(form.finishDate) }}
        {{ form_row(form.progress) }}
        <output id="textInput">0</output>
        {{ form_end(form) }}
    </div>
{% endblock %}
